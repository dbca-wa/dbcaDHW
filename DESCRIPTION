Package: dbcaDHW
Type: Package
Title: Functions to aid in analysing Sea Surface Temperature from Coral Reef Watch
Version: 0.1.0
Authors@R: person("Bart", "Huntley", email = "bart.huntley@dbca.wa.gov.au",
                  role = c("aut", "cre", "prg", "res"),
